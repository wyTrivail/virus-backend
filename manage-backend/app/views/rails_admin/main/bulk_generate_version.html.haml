%h4 '是否发布数据'
%ul= render partial: "version_notice", collection: @objects
= form_tag bulk_action_path(model_name: @abstract_model.to_param, bulk_ids: @objects.map(&:id)), method: :post do
  .form-actions
    %input{type: :hidden, name: 'return_to', value: (params[:return_to].presence || request.referer)}
    %input{type: :hidden, name: 'really', value: 'yes'}
    %input{type: :hidden, name: 'bulk_action', value: 'bulk_generate_version'}
    %button.btn.btn-danger{type: "submit", :'data-disable-with' => t("admin.form.confirmation")}
      %i.icon-white.icon-ok
      = t("admin.form.confirmation")
    %button.btn{type: "submit", name: "_continue", :'data-disable-with' => t("admin.form.cancel")}
      %i.icon-remove
      = t("admin.form.cancel")
